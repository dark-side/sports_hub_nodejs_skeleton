{
	"name": "nodejs_be_plgrnd",
	"version": "1.0.0",
	"description": "nodejs_be_plgrnd",
	"main": "dist/index.js",
	"engines": {
		"node": ">=22.14.0",
		"pnpm": ">=10.5.2"
	},
	"scripts": {
		"build": "tsc -b",
		"clean": "rm -rf dist",
		"prune": "rm -rf node_modules && pnpm clean",
		"start": "pnpm build && node dist/index.js",
		"dev": "tsx --watch src/index.ts",
		"test": "jest",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"check": "biome check .",
		"generate": "drizzle-kit generate:mysql",
		"migrate": "drizzle-kit push:mysql",
		"seed": "ts-node src/database/seeds/index.ts"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.29.5",
		"express": "^5.1.0",
		"js-yaml": "^4.1.0",
		"jsonwebtoken": "^9.0.2",
		"ms": "^2.1.3",
		"mysql2": "^3.14.0",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.1",
		"@types/js-yaml": "^4.0.9",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/ms": "^2.1.0",
		"@types/node": "^20.17.30",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.8",
		"drizzle-kit": "^0.20.14",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsx": "^4.19.3",
		"typescript": "^5.8.2"
	}
}
